<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">
  <servlet>
    <servlet-name>de.gishmo.mvp4g.example.multipresenterwithhistorie.Mvp4gMultiPresenterWithHistorieExample PersonService</servlet-name>
    <servlet-class>de.gishmo.mvp4g.example.multipresenterwithhistorie.server.PersonServiceImpl</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>de.gishmo.mvp4g.example.multipresenterwithhistorie.Mvp4gMultiPresenterWithHistorieExample HistoryContextService</servlet-name>
    <servlet-class>de.gishmo.mvp4g.example.multipresenterwithhistorie.server.HistoryContextServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>de.gishmo.mvp4g.example.multipresenterwithhistorie.Mvp4gMultiPresenterWithHistorieExample PersonService</servlet-name>
    <url-pattern>/mvp4gmultipresenterwithhistorieexample/person</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>de.gishmo.mvp4g.example.multipresenterwithhistorie.Mvp4gMultiPresenterWithHistorieExample HistoryContextService</servlet-name>
    <url-pattern>/mvp4gmultipresenterwithhistorieexample/history</url-pattern>
  </servlet-mapping>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Mvp4gMultiPresenterExample.html</welcome-file>
  </welcome-file-list>
</web-app>
